<SOURCE>@AbapCatalog.preserveKey: true
@ClientHandling.algorithm: #SESSION_VARIABLE
@AbapCatalog.sqlViewName: 'INGCCLSH2'
@AbapCatalog.compiler.compareFilter: true
@AccessControl.authorizationCheck: #CHECK
@EndUserText.label: 'Classification Class Superior Hierarchy for Key Date'
@VDM.viewType: #COMPOSITE
@VDM.lifecycle.contract.type: #SAP_INTERNAL_API

@ObjectModel.usageType.sizeCategory: #L
@ObjectModel.usageType.serviceQuality: #C
@ObjectModel.usageType.dataClass: #MIXED
define view I_ClfnClassSuperiorForKeyDate
  with parameters
    @Environment.systemField: #SYSTEM_DATE
    P_KeyDate : dats 
  as select from I_ClfnClass            as AncestorClass
      inner join I_ClfnClassHierarchy   as Hierarchy               
        on  Hierarchy.DescendantClassInternalID =  AncestorClass.ClassInternalID
        and Hierarchy.ValidityStartDate         &lt;= $parameters.P_KeyDate
        and Hierarchy.ValidityEndDate           &gt;= $parameters.P_KeyDate
        and AncestorClass.ValidityStartDate     &lt;= $parameters.P_KeyDate
        and AncestorClass.ValidityEndDate       &gt;= $parameters.P_KeyDate
      inner join I_ClfnClass            as DirectDescendantClass 
        on  DirectDescendantClass.ClassInternalID   =  Hierarchy.AncestorClassInternalID
        and DirectDescendantClass.ValidityStartDate &lt;= $parameters.P_KeyDate
        and DirectDescendantClass.ValidityEndDate   &gt;= $parameters.P_KeyDate        
{  
  key Hierarchy.DescendantClassInternalID   as ClassInternalID,
  key DirectDescendantClass.ClassInternalID as AncestorClassInternalID,
  key AncestorClass.ClassType               as ClassType,           
      AncestorClass.Class                   as Class,   
      DirectDescendantClass.Class           as AncestorClass,       
      AncestorClass.ClassClassfctnAuthGrp
}
