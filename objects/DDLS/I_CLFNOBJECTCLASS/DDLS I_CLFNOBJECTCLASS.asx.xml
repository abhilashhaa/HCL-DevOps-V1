<SOURCE>@AbapCatalog.preserveKey: true
 @ClientHandling.algorithm: #SESSION_VARIABLE
@AbapCatalog.sqlViewName: 'INGCCLFN9'
@AbapCatalog.compiler.compareFilter: true
@AccessControl.authorizationCheck: #CHECK
@EndUserText.label: 'Classification Object to Class'
@VDM.viewType: #COMPOSITE
@VDM.lifecycle.contract.type: #SAP_INTERNAL_API

@ObjectModel.usageType.sizeCategory: #L
@ObjectModel.usageType.serviceQuality: #C
@ObjectModel.usageType.dataClass: #MIXED
define view I_ClfnObjectClass
  as select from P_ClfnSingleObjectClass as Single  
      association [1..1] to I_ClfnClass as _Class 
        on $projection.ClassInternalID = _Class.ClassInternalID  
        and $projection.ValidityStartDate &lt;= _Class.ValidityEndDate 
        and $projection.ValidityEndDate   &gt;= _Class.ValidityStartDate          
{
  key Single.ClfnObjectID,
  key Single.ClfnObjectTable,
  key Single.ClassInternalID,
  key Single.ClassType,
  key Single.TimeIntervalNumber,
      '000000000000000000' as ClfnObjectInternalID,
      Single.ClfnStatus,
      Single.ClassPositionNumber,
      Single.ClassIsStandardClass,
      Single.BOMIsRecursive,
      Single.IsDeleted,
      Single.ChangeNumber,
      @Semantics.businessDate.from: true
      Single.ValidityStartDate,
      @Semantics.businessDate.to: true
      Single.ValidityEndDate,
      
      _Class
}  
  where ClfnObjectType = 'O' 

union

  select from P_ClfnMultipleObjectClass as Multiple
  
      association [1..1] to I_ClfnClass as _Class 
        on $projection.ClassInternalID = _Class.ClassInternalID  
        and $projection.ValidityStartDate &lt;= _Class.ValidityEndDate 
        and $projection.ValidityEndDate   &gt;= _Class.ValidityStartDate          
{
  key Multiple.ClfnObjectID,
  key Multiple.ClfnObjectTable,
  key Multiple.ClassInternalID,
  key Multiple.ClassType,
  key Multiple.TimeIntervalNumber,
      Multiple.ClfnObjectInternalID,
      Multiple.ClfnStatus,
      Multiple.ClassPositionNumber,
      Multiple.ClassIsStandardClass,
      Multiple.BOMIsRecursive,
      Multiple.IsDeleted,
      Multiple.ChangeNumber,
      @Semantics.businessDate.from: true
      Multiple.ValidityStartDate,
      @Semantics.businessDate.to: true
      Multiple.ValidityEndDate,
      
      _Class
}  
  where ClfnObjectType = 'O' 

