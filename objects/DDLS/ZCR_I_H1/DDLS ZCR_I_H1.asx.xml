@Search.searchable: true

@ObjectModel: {
compositionRoot: true,
modelCategory: #BUSINESS_OBJECT,
transactionalProcessingEnabled: true,
writeActivePersistence: 'ZCR_H1',
semanticKey: ['request_id'],
representativeKey: 'request_id',

createEnabled: true,
updateEnabled: true,
deleteEnabled: true
//draftEnabled: true,

//writeDraftPersistence: 'ZCR_D_H1'
}


define view ZCR_I_H1
  as select from zcr_h1
  association [*] to ZCR_I_C1 as _C1 on _C1.request_id = $projection.request_id
  association [*] to ZCR_I_S1 as _S1 on _S1.request_id = $projection.request_id
{

         //ZCR_HEADER
  key    request_id,
         title,
         @Search.defaultSearchElement: true
         cust_name1,
         cust_name2,
         country_key,
         city,
         postal_code,
         street,
         tel_num,
         sterm_1,
         sterm_2,
         created_by,
         created_on,
         created_at,
         @ObjectModel.association.type: [#TO_COMPOSITION_CHILD]
         _S1,
         @ObjectModel.association.type: [#TO_COMPOSITION_CHILD]
         _C1 // Make association public
}
