@EndUserText.label: 'CDS View with aggregation functions'
define view Zsup_view_003 as select from mara as a 
inner join mbew as b
on a.matnr = b.matnr
/*{

    b.bwkey as plant,
    count(*) as Material_total,
    count(distinct a.mtart) as Material_types,
    max(b.stprs) as max_price,
    min(b.stprs) as min_price,
    avg(b.lbkum) as avg_stock,
    sum(b.lbkum) as total_stock,
    avg(b.salk3) as avg_value,
    sum(b.salk3) as total_value
}
group by b.bwkey*/
{
b.bwkey as plant,
b.stprs as Std_price,
b.lbkum as stock,
b.salk3 as value
}
