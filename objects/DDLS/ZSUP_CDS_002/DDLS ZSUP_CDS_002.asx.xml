<SOURCE>@AbapCatalog.sqlViewName: 'ZSUP_CDS_002'
@AbapCatalog.compiler.compareFilter: true
@AbapCatalog.preserveKey: true
@AccessControl.authorizationCheck: #CHECK
@EndUserText.label: 'CDS View with Joins'
define view Zsup_view_002 as select from mara as a

// Get Material Type Text
inner join t134t as b
    on  b.mtart = a.mtart
    and b.spras = 'E'

// Get Industry Sector Text     
inner join t137t as c
    on  c.mbrsh = a.mbrsh
    and c.spras = 'E'

// Get Material Group Text    
inner join t023t as d
    on  d.matkl = a.matkl
    and d.spras = 'E'    
    
{    
    a.matnr as material_no,
    a.ersda as created_on,
    a.ernam as created_by,
    b.mtbez as material_typ,
    c.mbbez as industry,
    d.wgbez as material_grp   
}
