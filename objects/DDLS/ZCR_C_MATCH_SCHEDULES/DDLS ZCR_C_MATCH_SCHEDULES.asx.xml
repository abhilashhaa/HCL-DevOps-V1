@EndUserText.label: 'Match Schedules'
define view ZCR_C_MATCH_SCHEDULES
  as select from zcr_schedules
{
  key matchid          as MatchID,
      pair1            as Pair1ID,
      pair2            as Pair2ID,
      game_type        as GameType,
      set1_pair1_games as Set1Pair1Games,
      set1_pair2_games as Set1Pair2Games,
      set2_pair1_games as Set2Pair1Games,
      set2_pair2_games as Set2Pair2Games,
      champ_tieb_pair1 as ChampTieBreakPair1,
      champ_tieb_pair2 as ChampTieBreakPair2,
      points_home      as PointsHome,
      points_away      as PointsAway,
      pair1_won        as IsPair1Won,
      pair2_won        as IsPair2Won,
      match_on         as MatchOn,
      is_locked        as IsMatchLocked,
      created_at       as MatchCreatedOn,
      changed_at       as MatchChangedOn
}
