@ObjectModel.usageType.sizeCategory: #S
@ObjectModel.usageType.serviceQuality: #A
@ObjectModel.usageType.dataClass: #CUSTOMIZING
define view I_ClfnCharcStatus
  as select from tcms as CharacteristicStatus
  
      association [0..*] to I_ClfnCharcStatusText as _CharacteristicStatusText 
        on _CharacteristicStatusText.CharcStatus = $projection.CharcStatus
{
      @ObjectModel.text.association: '_CharacteristicStatusText'
  key CharacteristicStatus.atmst    as CharcStatus,
//      CharcStatus.atfre, Released
//      CharcStatus.atlvm, Deleted
//      CharcStatus.dlock, Distribution lock

      _CharacteristicStatusText
}
