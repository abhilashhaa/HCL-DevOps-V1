@EndUserText.label: 'Classification Organizational Area'
@VDM.viewType: #BASIC
@VDM.lifecycle.contract.type: #SAP_INTERNAL_API

@ObjectModel.representativeKey: 'ClfnOrganizationalAreaCode'
@ObjectModel.usageType.sizeCategory: #S
@ObjectModel.usageType.serviceQuality: #A
@ObjectModel.usageType.dataClass: #CUSTOMIZING
define view I_ClfnOrganizationalArea
  as select from tcls as OrganizationalArea

      association [0..*] to I_ClfnOrganizationalAreaText as _OrganizationalAreaText
        on  _OrganizationalAreaText.ClassType                  = $projection.ClassType
        and _OrganizationalAreaText.ClfnOrganizationalAreaCode = $projection.ClfnOrganizationalAreaCode
{
      @ObjectModel.foreignKey.association: null
      @ObjectModel.text.association: '_OrganizationalAreaText'
  key OrganizationalArea.klart as ClassType,
  key OrganizationalArea.sicht as ClfnOrganizationalAreaCode,

      _OrganizationalAreaText
}

