@Metadata.ignorePropagatedAnnotations: true
@EndUserText.label: 'TP for Classification Object Characteristic'

@AbapCatalog.preserveKey: true
define view I_ClfnObjectCharcTP
  with parameters
    @Consumption.hidden: true
    @Environment.systemField: #SYSTEM_DATE
    P_KeyDate : sydate
  as select distinct from I_ClfnObjectCharcForKeyDate(P_KeyDate: $parameters.P_KeyDate) as ObjectCharc

    composition [0..*] of I_ClfnObjectCharcValueTP as _ObjectCharcValue

    association to parent I_ClfnObjectTP as _Object on
      $projection.ClfnObjectID    = _Object.ClfnObjectID and
      $projection.ClfnObjectTable = _Object.ClfnObjectTable

  {
   key ObjectCharc.ClfnObjectID,
   key ObjectCharc.ClfnObjectTable,
   key ObjectCharc.ClassType,
   key ObjectCharc.CharcInternalID,

    ObjectCharc.ClfnObjectInternalID,
    
//    case when ObjectCharc.LastChangeDateTime = 0
//      then cast ( dats_tims_to_tstmp(cast ('19720401' as dats), cast ('000000' as tims ), abap_system_timezone( $session.client,'NULL' ), $session.client, 'NULL') as timestampl )
//      else ObjectCharc.LastChangeDateTime
//    end as LastChangeDateTime,

    ObjectCharc._Characteristic,

    _Object,
    _ObjectCharcValue
  }
  where
    ObjectCharc.ClfnObjectType  = 'O'
