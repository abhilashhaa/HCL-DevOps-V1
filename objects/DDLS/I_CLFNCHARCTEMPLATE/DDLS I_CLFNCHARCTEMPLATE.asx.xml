@AccessControl.authorizationCheck: #NOT_REQUIRED
@VDM.viewType: #BASIC
@VDM.lifecycle.contract.type: #SAP_INTERNAL_API
@EndUserText.label: 'Classification Characteristic Template'

@ObjectModel.representativeKey: 'CharcTmplPositionNumber'
@ObjectModel.usageType.sizeCategory: #S
@ObjectModel.usageType.serviceQuality: #A
@ObjectModel.usageType.dataClass: #CUSTOMIZING
define view I_ClfnCharcTemplate as select from tcpa as CharcTemplate

  association [0..*] to I_ClfnCharcTemplateText as _CharcTemplateText 
    on _CharcTemplateText.CharcTmplPositionNumber = $projection.CharcTmplPositionNumber

{
      @ObjectModel.text.association: '_CharcTemplateText'
  key CharcTemplate.patct   as CharcTmplPositionNumber,
      CharcTemplate.atfor   as CharcDataType,     
      CharcTemplate.atei6   as UnitOfMeasure, 
      CharcTemplate.atsch   as CharcTemplate,
      
      _CharcTemplateText
}
