@AccessControl.authorizationCheck: #NOT_REQUIRED
@EndUserText.label: 'Header Consumption'
@Metadata.allowExtensions: true
@OData.publish: true
@Search.searchable: true

@ObjectModel.transactionalProcessingDelegated: true
@ObjectModel.semanticKey: ['UUID']
@ObjectModel.representativeKey: 'UUID'
@ObjectModel.createEnabled: true
@ObjectModel.updateEnabled: true
@ObjectModel.deleteEnabled: true

//@ui.headerInfo.typeName: 'Customer'
//@ui.headerInfo.title.value: 'UUID'

define view ZCR_C_HEADER
//as select from kna1
  as select from ZCR_I_H
  association [*] to ZCR_C_CONTACTS   as _contacts  on _contacts.uuid = $projection.uuid
  association [*] to ZCR_C_SALES_ORGS as _SalesOrgs on _SalesOrgs.uuid = $projection.uuid
{
      //zcr_header
  key uuid, 
  request_id,
      title,
      @Search.defaultSearchElement: true
      @Search.fuzzinessThreshold: 0.9
      cust_name1,
      @Search.defaultSearchElement: true
      @Search.fuzzinessThreshold: 0.9
      cust_name2,
      country_key,
      city,
      postal_code,
      street,
      tel_num,
      sterm_1,
      sterm_2,
      created_by,
      created_on,
      created_at,

      @Search.defaultSearchElement: true
      _contacts,

      @Search.defaultSearchElement: true
      _SalesOrgs
}
