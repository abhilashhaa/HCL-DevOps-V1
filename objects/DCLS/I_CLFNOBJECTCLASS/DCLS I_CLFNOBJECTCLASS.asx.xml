<SOURCE>@EndUserText.label: 'Mapping Role for I_ClfnObjectClass'
@MappingRole: true
define role I_ClfnObjectClass{
  grant select on I_ClfnObjectClass
    // ClassType authorization    
      where ( ClassType ) =  aspect pfcg_auth ( C_TCLA_BKA, KLART )                       
    // Classification authorization
      and ( ( _Class.ClassClassfctnAuthGrp =  '' and                               ( ) =  aspect pfcg_auth (  C_KLAH_BKL, ACTVT = '03' ) )          //display                                                           
        or  ( _Class.ClassClassfctnAuthGrp &lt;&gt; '' and  ( _Class.ClassClassfctnAuthGrp ) =  aspect pfcg_auth (  C_KLAH_BKL, BGRKL, ACTVT = '03' ) ) ) //display      
    ;                                 
}
</SOURCE>
