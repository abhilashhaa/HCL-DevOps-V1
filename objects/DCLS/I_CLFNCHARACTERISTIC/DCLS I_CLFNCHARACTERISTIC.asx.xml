<SOURCE>@EndUserText.label: 'Mapping role for I_ClfnCharacteristic'
@MappingRole: true
define role I_ClfnCharacteristic {
  grant select on I_ClfnCharacteristic
    // Characteristic authorization
    where ( 
            ( CharcMaintAuthGrp = ''  and 
            ( ) =  aspect pfcg_auth (  C_CABN, ACTVT = '03' ) //display
          ) 
    or    (  CharcMaintAuthGrp &lt;&gt; ''  and 
            ( CharcMaintAuthGrp )  = aspect pfcg_auth ( C_CABN_GRP, ATAUTH ) and
            ( ) =  aspect pfcg_auth (  C_CABN, ACTVT = '03' ) ) //display 
          ); 
}
</SOURCE>
