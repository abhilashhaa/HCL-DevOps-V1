    aspect pfcg_auth (  M_MATE_MAT,
                        BEGRU,
                        actvt = '03' )
    and ( _ProductType.AuthorizationGroup  )   ?= aspect pfcg_auth ( M_MATE_MAR, BEGRU, actvt = '03' ) 
   
    and ( _ProductGroup.AuthorizationGroup )   ?= aspect pfcg_auth ( M_MATE_WGR, begru, actvt = '03' ); 
                                                                                                                                                                  
}</SOURCE>
