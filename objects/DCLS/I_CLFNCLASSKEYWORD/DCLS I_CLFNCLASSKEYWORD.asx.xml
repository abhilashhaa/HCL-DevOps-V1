<SOURCE>@EndUserText.label: 'Mapping Role for I_ClfnClassKeyword'
@MappingRole: true
define role I_ClfnClassKeyword{
  grant select on I_ClfnClassKeyword   
    // ClassType authorization       
      where ( ) =  aspect pfcg_auth (  C_KLAH_BKP, ACTVT = '03' )  //display
    // Class authorization              
      and ( ( _Class.ClassMaintAuthGrp = '' and 
            ( ) =  aspect pfcg_auth (  C_KLAH_BKP, ACTVT = '03' ) )  //display                                                           
         or 
          ( _Class.ClassMaintAuthGrp &lt;&gt; '' and  
          ( _Class.ClassMaintAuthGrp ) =  aspect pfcg_auth (  C_KLAH_BKP, BGRKP, ACTVT = '03' ) )  //display   
    );                                                
}
</SOURCE>
