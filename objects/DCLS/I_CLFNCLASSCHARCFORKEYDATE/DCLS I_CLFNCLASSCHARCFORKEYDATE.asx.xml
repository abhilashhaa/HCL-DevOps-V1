<SOURCE>@EndUserText.label: 'Mapping Role for I_ClfnClassCharcForKeyDate'
@MappingRole: true
define role I_ClfnClassCharcForKeyDate{
  grant select on I_ClfnClassCharcForKeyDate
    // ClassTye authorization
      where ( ClassType ) =  aspect pfcg_auth ( C_TCLA_BKA, KLART )
    // Class authorization
      and ( (  _Class.ClassMaintAuthGrp = '' and
            ( ) =  aspect pfcg_auth (  C_KLAH_BKP, ACTVT = '03' ) )  //display
          or
            ( _Class.ClassMaintAuthGrp &lt;&gt; '' and
            ( _Class.ClassMaintAuthGrp ) =  aspect pfcg_auth (  C_KLAH_BKP, BGRKP, ACTVT = '03' ) ) ) //display
  ;
}
