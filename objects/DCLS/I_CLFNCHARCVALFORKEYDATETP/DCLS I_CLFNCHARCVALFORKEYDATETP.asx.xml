<SOURCE>@EndUserText.label: 'Mapping role for I_ClfnCharcValForKeyDateTP' 
@MappingRole: true 
define role I_ClfnCharcValForKeyDateTP { 
  grant select on I_ClfnCharcValForKeyDateTP 
    // Characteristic authorization
      where ( 
            ( _Characteristic( P_KeyDate : $parameters.P_KeyDate ).CharcMaintAuthGrp = ''  and 
            ( ) =  aspect pfcg_auth (  C_CABN, ACTVT = '03' ) //display
          )
      or    (  _Characteristic( P_KeyDate : $parameters.P_KeyDate ).CharcMaintAuthGrp &lt;&gt; ''  and 
            ( _Characteristic( P_KeyDate : $parameters.P_KeyDate ).CharcMaintAuthGrp )  = aspect pfcg_auth ( C_CABN_GRP, ATAUTH ) and
            ( ) =  aspect pfcg_auth (  C_CABN, ACTVT = '03' ) ) //display 
          ); 
}</SOURCE>
